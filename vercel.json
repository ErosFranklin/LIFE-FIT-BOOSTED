{
    "version": 2,
    "routes": [
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/([^/]+)\\.html",
        "dest": "/public/$1.html"
      },
      {
        "src": "/([^/]+)",
        "dest": "/public/views/$1.html"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
}